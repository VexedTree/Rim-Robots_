<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Vanilla Aspirations Expanded</li>
    </mods>
	<match Class="PatchOperationSequence">
		<operations>
		    <li Class="PatchOperationAdd">
				<xpath>/Defs/VAspirE.AspirationDef[defName="VAspirE_KillSomeone"]/requiredTraitsAny</xpath>
				<value>
					<Anthropophobia>1</Anthropophobia>
				</value>
			</li>			
		</operations>
	</match>
  </Operation>  
</Patch>