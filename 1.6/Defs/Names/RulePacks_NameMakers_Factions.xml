  <RulePackDef>
    <defName>NamerSettlementSynthetic</defName>
    <rulePack>
        <rulesStrings>
          <li>r_name(p=5)->[synthhaven]</li>
          <li>r_name(p=3)->[synthhaven_combo]</li>
          <li>r_name->[synthhaven_near]</li>

		  <li>synthtopic->[Adjective]</li>
          <li>synthtopic->[Place]</li>
		  <li>synthtopic->[Color]</li>

          <li>synthhaven(p=2)->[SynthSyll][SynthSyllE]</li>
          <li>synthhaven(p=2)->[SynthSyll][SynthSyllE][SynthSyllE]</li>
          <li>synthhaven(p=2)->[SynthSyll][SynthSyllE]</li>
		  <li>synthhaven->[SynthSyll][SynthSyllE] [Letter]</li>
		  <li>synthhaven->[SynthSyll][SynthSyllE] [Letter][Letter]</li>
          <li>synthhaven->[SynthSyll][SynthSyllE][SynthSyllE] [Letter]</li>
		  <li>synthhaven->[SynthSyll][SynthSyllE][SynthSyllE] [Letter][Letter]</li>
		  
          <li>synthhaven_combo->[synthtopic] [Place]</li>
          <li>synthhaven_combo->[synthtopic] [SynthCITY]</li>
          <li>synthhaven_combo->[concept_positive]'s [Place]</li>
          <li>synthhaven_combo->[concept_positive] [Place]</li>
          <li>synthhaven_combo(p=2)->[SynthPerson]'s [Place]</li>
		  <li>synthhaven_combo(p=2)->[SynthPerson]'s [SynthCITY]</li>
 
          <li>synthhaven_near->[SynthCITY]-at-the-[Place]</li>
          <li>synthhaven_near->[SynthCITY]-in-the-[Place]</li>
          <li>synthhaven_near->[SynthCITY]-on-the-[Place]</li>
        </rulesStrings>
			<rulesRaw>	
                <li Class="Rule_File">
                    <keyword>SynthSyll</keyword>
