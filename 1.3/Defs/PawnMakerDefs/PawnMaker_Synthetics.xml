<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!-- Mechanents -->
    <ThingDef>
        <defName>Printed_Mechanent_Spawner</defName>
        <label>mechanent</label>
        <description>Assembles a new mechanent.</description>
        <thingClass>Building</thingClass>
        <statBases>
            <WorkToMake>550</WorkToMake>
            <MaxHitPoints>150</MaxHitPoints>
            <Flammability>0.1</Flammability>
        </statBases>
        <graphicData>
            <texPath>Buildings/Blank/Null</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
        </graphicData>		
        <size>(1,1)</size>
        <category>Building</category>
        <pathCost>200</pathCost>
        <passability>Standable</passability>
        <filthLeaving>Filth_RubbleRock</filthLeaving>
        <hasInteractionCell>false</hasInteractionCell>
        <drawerType>MapMeshAndRealTime</drawerType>
        <altitudeLayer>Building</altitudeLayer>
        <repairEffect>Repair</repairEffect>
        <useHitPoints>True</useHitPoints>
        <selectable>true</selectable>
        <surfaceType>Item</surfaceType>
        <tickerType>Normal</tickerType>
        <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
        <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
        <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>		
        <comps>
            <li Class="Hivers.CompProperties_HatcherMulti">
                <hatcherDaystoHatch>0</hatcherDaystoHatch>
                <hatcherPawns>
				    <li>Printed_Mechanent</li>
				</hatcherPawns>
            </li>
        </comps>		
    </ThingDef>	

