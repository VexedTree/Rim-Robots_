<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <RulePackDef>
    <defName>NamerPersonSynthNonGendered</defName>
    <rulePack>
      <rulesStrings>
        <li>name(p=2)->[NameSynth]</li>
        <li>name(p=2)->[SylStart][SylEnd]</li>		
        <li>name(p=2)->[NameSynth] [Color]</li>		
		<li>name(p=2)->[NameSynth] [Animal]</li>		
        <li>name(p=2)->[NameSynth] [Place]</li>		
        <li>name(p=2)->[NameSynth] the [Adjectives]</li>	
        <li>name(p=2)->[NameSynth] [Adjectives]</li>	
		<li>name(p=3)->[Objects][VerbSynth]</li>	
		<li>name->[Color]</li>	
		<li>name->[Color] [Place]</li>
		<li>name->[Color] [Adjectives]</li>				
		<li>name->[Animal]</li>
		<li>name->[Place]</li>				
        <li>name(p=2)->[Adjectives]</li>
		<li>name(p=2)->[Adjectives] [Color]</li>
		<li>name(p=2)->[Adjectives] [Animal]</li>
		<li>name(p=2)->[Adjectives] [Place]</li>		
        <li>name->[Letter][Number]</li>
		<li>name->[Letter][Number][Number]</li>
		<li>name->[Letter][Number][Number][Number]</li>
		<li>name->[Letter][Letter][Number]</li>
		<li>name->[Letter][Letter][Number][Number]</li>
		<li>name->[Letter][Letter][Number][Number][Number]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>NameSynth</keyword>
          <path>Synth_Names/Names/NameRobotic</path>
        </li>
        <li Class="Rule_File">
            <keyword>Adjectives</keyword>
            <path>Synth_Names/AdjectivesRobot</path>
        </li>	
        <li Class="Rule_File">
            <keyword>Color</keyword>
            <path>Titles/Colors</path>
        </li>	
        <li Class="Rule_File">
            <keyword>Animal</keyword>
            <path>Titles/Animals</path>
        </li>	
        <li Class="Rule_File">
            <keyword>Place</keyword>
            <path>Titles/Places</path>
        </li>
        <li Class="Rule_File">
            <keyword>Objects</keyword>
            <path>Synth_Names/ObjectsRobot</path>
        </li>		
        <li Class="Rule_File">
          <keyword>VerbSynth</keyword>
          <path>Synth_Names/VerbsRobot</path>
        </li>	
        <li Class="Rule_File">
            <keyword>Letter</keyword>
            <path>Titles/Letters</path>
        </li>
        <li Class="Rule_File">
            <keyword>Number</keyword>
            <path>Titles/Numbers</path>
        </li>
        <li Class="Rule_File">
            <keyword>SylStart</keyword>
            <path>Synth_Syllables/SynthName_Syll</path>
        </li>
        <li Class="Rule_File">
            <keyword>SylEnd</keyword>
            <path>Synth_Syllables/SynthName_SyllEnd</path>
        </li>		
      </rulesRaw>
    </rulePack>
  </RulePackDef>

